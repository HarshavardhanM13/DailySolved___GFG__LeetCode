Valid Matrix Sum

import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n,m;
        n = sc.nextInt();
        m = sc.nextInt();
        int arr[][] = new int[n][m];
        if((n * m) % 2 == 1){
            System.out.println(-1);
        }
        else{
            for(int i=0;i<n;i++){
                for(int j=0;j<m;j++){
                    System.out.print(1 + " ");
                }
                System.out.print("\n");
            }
        }
    }
}
